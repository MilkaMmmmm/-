var students = ["Макар", "Степа", "Глеб", "Рома", "Ваня", "Антон", "Леша", "Володя", "Михаил", "Растислав", "Аркадий", "Влад"]

print("Список учеников:")
for student in students {
    print(student)
}

students.append("Лиза")
students.append("Алена")
students.append("Катя")
students.insert("Таня", at: 3)
students.insert("Даша", at: 6)
students.insert("Диана", at: 9)
students.insert("Софа", at: 12)

print("\nОбновленный список учеников:")
for student in students {
    print(student)
}
