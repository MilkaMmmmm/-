var students = ["дима", "степа", "даня", "рома", "ваня", "антон", "макар", "сергей", "миша", "вова", "максим", "влад"]

print("Список учеников:")
for student in students {
    print(student)
}

students.append("настя")
students.append("алена")
students.append("соня")
students.insert("таня", at: 3)
students.insert("даша", at: 6)
students.insert("лиза", at: 9)
students.insert("вика", at: 12)

print("\nОбновленный список учеников:")
for student in students {
    print(student)
}
