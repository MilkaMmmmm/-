var students = ["Леня", "Антон", "Валера", "Александр", "Фофа", "Максим", "Дима", "Леша", "Адриан", "Данон", "Ерема", "Анатолий", "Егор", "Лиза", "Геля", "Нина", "Даша", "Глаша", "Вики"]

print("Исходный список учеников:")
for student in students {
    print(student)
}

students.removeSubrange(0...6)

print("\nОбновленный список учеников:")
for student in students {
    print(student)
}
