var students = ["леня", "антон", "даня", "рома", "фофа", "степа", "дима", "сергей", "миша", "данон", "максим", "влад", "ляля", "катя", "соня", "настя", "даша", "лиза", "вика"]

print("Исходный список учеников:")
for student in students {
    print(student)
}

students.removeSubrange(0...6)

print("\nОбновленный список учеников:")
for student in students {
    print(student)
}
