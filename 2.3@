let line1 = "Очень тяжело,"
let line2 = "но мы справились."
let line3 = "Сидим дома "
let line4 = "пьем чай."
let line5 = "На улице не очень погода"
let line6 = "и единственное,о чем думаешь:"
let line7 = "скорее бы солнце,"
let line8 = "вот блин\n\tзачем нам этот дождь??"
let line9 = "А Даша сказала: \"Мила,не расстаивайся ,"
let line10 = "завтра будет солнце!\""

let poem = """
\(line1)
\(line2)
\(line3)
\(line4)

\(line5)
\(line6)
\(line7)
\(line8)

\(line9)
\(line10)
"""

print(poem)
