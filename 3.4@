struct Student {
    let name: String
    let grade: Int
    
   
    func getGradeDescription() -> String {
        switch grade {
        case 0...2:
            return "Плохо"
        case 3...4:
            return "Нормально"
        case 5:
            return "Неплохо"
        default:
            return "Отлично"
        }
    }
}

let students = [
    Student(name: "Рома", grade: 5),
    Student(name: "Ваня", grade: 3),
    Student(name: "Соня", grade: 4),
    Student(name: "Влад", grade: 2), 
    Student(name: "Антон", grade:4)
]


for student in students {
    let description = "\(student.name) - \(student.grade) баллов. \(student.getGradeDescription())."
    print(description)
}
